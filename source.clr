
#include stdio.h stdlib.h
#define N 10000

int: a # N

void: gen_random <- int: uppper_bound

  for i <- 0, 1.. N-1
    a[i] = rand() % uppper_bound

int: howmany <- int: value
  int: count = 0
  for i <- 0, 1.. N-1
    if a[i] == value
      count += 1
  => count

int: main <- void:
  gen_random 10
  printf "value \t how many\n"
  for i <- 0, 1.. 9
    printf "%d \t %d \n", i, howmany(i)

  => 0